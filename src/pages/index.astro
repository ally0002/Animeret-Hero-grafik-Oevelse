---
import MainLayout from "../layouts/MainLayout.astro";
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import thumbsup from "../assets/images/thumbsup.png";
import Sektioner from '../components/Sektioner.astro';
import Header from '../components/Header.astro';
import Footer from "../components/Footer.astro";
import Herounit from "../components/Herounit.astro";
---
<MainLayout title="Titel">
	<article>
		<Header />
		<h1>Index overskrift</h1>
		<Button>Klik!</Button>
		<p>Link til <Link href="https://www.kea.dk" target="_blank">En anden side</Link></p>
		<Image src={thumbsup} alt=""></Image>
		<Herounit />
		<Sektioner />
	</article>
</MainLayout>